<h3> <%= @marker.first_name %> </h3>
<button class="btn" onclick="location.href='/login/show'">Logout</button>
<table class="striped">
    <thead>
        <tr style="">
            <th> <i class="material-icons">assignment</i>   Courses </th>
        </tr>
    </thead>
    <tbody>
        <% @courses.each do |course| %>
            <tr>
                <td> <%= link_to course.long_title, marker_course_path(@marker.id, course) %> </td>
            </tr>
        <% end %>
    </tbody>

</table>